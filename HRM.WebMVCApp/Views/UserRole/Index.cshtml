@using HRM.ApplicationCore.Model.Response;
@model IEnumerable<UserRoleResponseModel>;
<div class="page-header">
    <h1>List of Feedback</h1>
</div>
<a asp-action="Create" asp-controller="UserRole">Add New UserRole</a>
<table class="table table-bordered table-striped table-hovered">
    <thead>
        <tr>
            <th>UserId</th>
            <th>RoleId</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var m in Model)
            {
                        <tr>
                            <td>@m.UserId</td>
                            <td>@m.RoleId</td>
                            <td><a asp-action="Edit" asp-controller="UserRole" asp-route-id="@m.Id">Edit</a></td>
                            <td><a asp-action="Delete" asp-controller="UserRole" asp-route-id="@m.Id">Delete</a></td>
                        </tr>
            }
        }
    </tbody>
</table>